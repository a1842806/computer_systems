/**
 * 16-bit incrementer:
 * out = in + 1 (arithmetic addition)
 */

CHIP Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
   // Put you code here:
   Xor(a=in[0], b=in[0], out=out1);
   Add16(a=in, b=out1, out=out, over=over);
}